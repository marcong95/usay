
   <!-- Left side column. contains the logo and sidebar -->
 <aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
      <!-- Sidebar user panel -->
      <div class="user-panel">
        <div class="pull-left image">
          <img src="<%= manager.avator %>" class="img-circle" alt="User Image">
        </div>
        <div class="pull-left info">
          <p><%= manager.username %></p>
          <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
        </div>
      </div>
      <!-- sidebar menu: : style can be found in sidebar.less -->
      <ul class="sidebar-menu">

        <% var sidebar = config.right; %>
        <% for(var i in sidebar){ var menu = sidebar[i]; %><!-- for1 -->
          <li class="header"><%= menu.header %></li>
          <% if(menu.list){ (function(list){  if(!list) return; %><!-- if1 and function -->
            <% for(var i in list){ var item = list[i];%><!-- for2 -->
              <% if(item.list){ %>
              <li class="treeview">
                <a href="#">
                  <i class="fa <%= item.icon %>"></i> <span><%= item.title %></span>
                  <span class="pull-right-container">
                    <i class="fa fa-angle-left pull-right"></i>
                  </span>
                </a>
                <ul class="treeview-menu">
                  <%  arguments.callee(item.list); %>
                </ul>
              </li>
              <% }else{ %>
                <li class="main-link">
                    <a href="javascript:void(0)" data-role="<%= item.data_role %>" class="main-block">
                        <i class="fa <%= item.icon %>"></i> <span class="title"><%= item.title %></span>
                    </a>
                </li>
              <% } %>
            <% } %><!-- for2 end -->
          <% })(menu.list); }%><!-- if1 and function end -->
        <% } %><!-- for1 end -->
      </ul>
    </section>
  </aside>
